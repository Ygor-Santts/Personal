#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules/@nuxt/devtools/node_modules:/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules/@nuxt/node_modules:/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules:/home/user/Personal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules/@nuxt/devtools/node_modules:/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules/@nuxt/node_modules:/home/user/Personal/node_modules/.pnpm/@nuxt+devtools@2.6.5_vite@7.1.7_@types+node@24.5.2_jiti@2.6.0_lightningcss@1.30.1_terse_43413c97f5a612ce957f561533064a28/node_modules:/home/user/Personal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
fi
